@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

# Find required dependencies
find_dependency(fmt REQUIRED)
find_dependency(OpenSSL REQUIRED)
find_dependency(boost REQUIRED)
find_dependency(protobuf REQUIRED)
find_dependency(spdlog REQUIRED)
find_dependency(rocksdb REQUIRED)
find_dependency(gtest REQUIRED)

# Include the targets file
include("${CMAKE_CURRENT_LIST_DIR}/ChainForgeTargets.cmake")

# Set package properties
set(ChainForge_VERSION "@ChainForge_VERSION@")
set(ChainForge_VERSION_MAJOR "@ChainForge_VERSION_MAJOR@")
set(ChainForge_VERSION_MINOR "@ChainForge_VERSION_MINOR@")
set(ChainForge_VERSION_PATCH "@ChainForge_VERSION_PATCH@")

check_required_components(ChainForge)
